{
    "name": "project-tasks",
    "displayName": "Project Tasks",
    "description": "Run tasks from a status bar button",
    "version": "1.0.0",
    "publisher": "haugerbr",
    "license": "MIT",
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "keywords": [
        "tasks"
